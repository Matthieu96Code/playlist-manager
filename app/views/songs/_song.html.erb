<div id="<%= dom_id song %>">
  <p>
    <strong>Name:</strong>
    <%= song.name %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= song.title %>
  </p>

  <p>
    <strong>Artist:</strong>
    <%= song.artist %>
  </p>

  <p>
    <strong>Key:</strong>
    <%= song.key %>
  </p>

  <p>
    <strong>Mastered:</strong>
    <%= song.mastered %>
  </p>

</div>
